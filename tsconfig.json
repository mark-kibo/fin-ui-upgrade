{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    // "types": ["json5"],
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/components/dashlayout.jsx", "src/components/ApexCharts/AreaChart.jsx", "src/components/ApexCharts/barChart.jsx", "src/components/ApexCharts/ColumnChart.jsx", "src/components/ApexCharts/PieChart.jsx", "src/components/SideBarContent.tsx", "src/components/SideNavContainer/Subcontainer.jsx", "src/context/SideBarContext.tsx"],
  "exclude": ["node_modules"]
}
